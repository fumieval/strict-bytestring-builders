#!/bin/bash
set -eo pipefail

results_dir=dist/results
mkdir -p "$results_dir"

function benchmark {
  dist/build/benchmark/benchmark -o "$results_dir/$1.html" "$1/"
}

benchmark "averaged-appends-1"
benchmark "averaged-appends-100"
benchmark "averaged-appends-10000"
benchmark "regular-concat-100"
benchmark "regular-concat-10000"
